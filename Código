    private void btnborrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        txtvalorA.setText("");
        txtvalorB.setText("");
        txtvalorC.setText("");
        txtsolucion1.setText("");
        txtsolucion2.setText("");
    }                                         

    private void btnsalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        dispose();

    }                                        

    private void btncalcularActionPerformed(java.awt.event.ActionEvent evt) {                                            
        double valorA, valorB, valorC;
        double punto1, punto2;
        
        valorA = Double.parseDouble(txtvalorA.getText());
        valorB = Double.parseDouble(txtvalorB.getText());
        valorC = Double.parseDouble(txtvalorC.getText());
        punto1 = Ecuacion.calcEcuacion1(valorA, valorB, valorC);       
        punto2 = Ecuacion.calcEcuacion2(valorA, valorB, valorC);
        
        txtsolucion1.setText(String.valueOf(punto1));
        txtsolucion2.setText(String.valueOf(punto2));


    }                                           
