Clase:
package ejer.pkg23.act3;
/**
 *
 * @author USER
 */
public class Ecuacion {
    public static double calcEcuacion1(double A, double B, double C){
    double punto_x1;
    punto_x1 = (-B + (Math.pow((B*B)-(4*A*C),0.5)))/(2*A);
    return punto_x1;
}
    public static double calcEcuacion2(double A, double B, double C){
    double punto_x2;
    punto_x2 = (-B - (Math.pow(((B*B)-(4*A*C)),0.5)))/(2*A);
    return punto_x2;
}
}
